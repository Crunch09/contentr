#contentr-fancybox-loading {
  position: fixed;
  top: 50%;
  left: 50%;
  width: 40px;
  height: 40px;
  margin-top: -20px;
  margin-left: -20px;
  cursor: pointer;
  overflow: hidden;
  z-index: 1104;
  display: none;
}

#contentr-fancybox-loading div {
  position: absolute;
  top: 0;
  left: 0;
  width: 40px;
  height: 480px;
  background-image: url(<%= asset_path('contentr/fancybox.png') %>);
}

#contentr-fancybox-overlay {
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  z-index: 1100;
  display: none;
}

#contentr-fancybox-tmp {
  padding: 0;
  margin: 0;
  border: 0;
  overflow: auto;
  display: none;
}

#contentr-fancybox-wrap {
  position: absolute;
  top: 0;
  left: 0;
  padding: 20px;
  z-index: 1101;
  outline: none;
  display: none;
}

#contentr-fancybox-outer {
  position: relative;
  width: 100%;
  height: 100%;
  background: #fff;
}

#contentr-fancybox-content {
  width: 0;
  height: 0;
  padding: 0;
  outline: none;
  position: relative;
  overflow: hidden;
  z-index: 1102;
  border: 0px solid #fff;
}

#contentr-fancybox-hide-sel-frame {
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  background: transparent;
  z-index: 1101;
}

#contentr-fancybox-close {
  position: absolute;
  top: -15px;
  right: -15px;
  width: 30px;
  height: 30px;
  background: transparent url(<%= asset_path('contentr/fancybox.png') %>) -40px 0px;
  cursor: pointer;
  z-index: 1103;
  display: none;
}

#contentr-fancybox-error {
  color: #444;
  font: normal 12px/20px Arial;
  padding: 14px;
  margin: 0;
}

#contentr-fancybox-img {
  width: 100%;
  height: 100%;
  padding: 0;
  margin: 0;
  border: none;
  outline: none;
  line-height: 0;
  vertical-align: top;
}

#contentr-fancybox-frame {
  width: 100%;
  height: 100%;
  border: none;
  display: block;
}

#contentr-fancybox-left, #contentr-fancybox-right {
  position: absolute;
  bottom: 0px;
  height: 100%;
  width: 35%;
  cursor: pointer;
  outline: none;
  background: transparent;
  z-index: 1102;
  display: none;
}

#contentr-fancybox-left {
  left: 0px;
}

#contentr-fancybox-right {
  right: 0px;
}

#contentr-fancybox-left-ico, #contentr-fancybox-right-ico {
  position: absolute;
  top: 50%;
  left: -9999px;
  width: 30px;
  height: 30px;
  margin-top: -15px;
  cursor: pointer;
  z-index: 1102;
  display: block;
}

#contentr-fancybox-left-ico {
  background-image: url(<%= asset_path('contentr/fancybox.png') %>);
  background-position: -40px -30px;
}

#contentr-fancybox-right-ico {
  background-image: url(<%= asset_path('contentr/fancybox.png') %>);
  background-position: -40px -60px;
}

#contentr-fancybox-left:hover, #contentr-fancybox-right:hover {
  visibility: visible; /* IE6 */
}

#contentr-fancybox-left:hover span {
  left: 20px;
}

#contentr-fancybox-right:hover span {
  left: auto;
  right: 20px;
}

.contentr-fancybox-bg {
  position: absolute;
  padding: 0;
  margin: 0;
  border: 0;
  width: 20px;
  height: 20px;
  z-index: 1001;
}

#contentr-fancybox-bg-n {
  top: -20px;
  left: 0;
  width: 100%;
  background-image: url(<%= asset_path('contentr/fancybox_x.png') %>);
}

#contentr-fancybox-bg-ne {
  top: -20px;
  right: -20px;
  background-image: url(<%= asset_path('contentr/fancybox.png') %>);
  background-position: -40px -162px;
}

#contentr-fancybox-bg-e {
  top: 0;
  right: -20px;
  height: 100%;
  background-image: url(<%= asset_path('contentr/fancybox_y.png') %>);
  background-position: -20px 0px;
}

#contentr-fancybox-bg-se {
  bottom: -20px;
  right: -20px;
  background-image: url(<%= asset_path('contentr/fancybox.png') %>);
  background-position: -40px -182px;
}

#contentr-fancybox-bg-s {
  bottom: -20px;
  left: 0;
  width: 100%;
  background-image: url(<%= asset_path('contentr/fancybox_x.png') %>);
  background-position: 0px -20px;
}

#contentr-fancybox-bg-sw {
  bottom: -20px;
  left: -20px;
  background-image: url(<%= asset_path('contentr/fancybox.png') %>);
  background-position: -40px -142px;
}

#contentr-fancybox-bg-w {
  top: 0;
  left: -20px;
  height: 100%;
  background-image: url(<%= asset_path('contentr/fancybox_y.png') %>);
}

#contentr-fancybox-bg-nw {
  top: -20px;
  left: -20px;
  background-image: url(<%= asset_path('contentr/fancybox.png') %>);
  background-position: -40px -122px;
}

#contentr-fancybox-title {
  font-family: Helvetica;
  font-size: 12px;
  z-index: 1102;
}

.contentr-fancybox-title-inside {
  padding-bottom: 10px;
  text-align: center;
  color: #333;
  background: #fff;
  position: relative;
}

.contentr-fancybox-title-outside {
  padding-top: 10px;
  color: #fff;
}

.contentr-fancybox-title-over {
  position: absolute;
  bottom: 0;
  left: 0;
  color: #FFF;
  text-align: left;
}

#contentr-fancybox-title-over {
  padding: 10px;
  /*background-image: url('fancy_title_over.png');*/
  display: block;
}

.contentr-fancybox-title-float {
  position: absolute;
  left: 0;
  bottom: -20px;
  height: 32px;
}

#contentr-fancybox-title-float-wrap {
  border: none;
  border-collapse: collapse;
  width: auto;
}

#contentr-fancybox-title-float-wrap td {
  border: none;
  white-space: nowrap;
}

#contentr-fancybox-title-float-left {
  padding: 0 0 0 15px;
  background: url(<%= asset_path('contentr/fancybox.png') %>) -40px -90px no-repeat;
}

#contentr-fancybox-title-float-main {
  color: #FFF;
  line-height: 29px;
  font-weight: bold;
  padding: 0 0 3px 0;
  background: url('fancybox-x.png') 0px -40px;
}

#contentr-fancybox-title-float-right {
  padding: 0 0 0 15px;
  background: url(<%= asset_path('contentr/fancybox.png') %>) -55px -90px no-repeat;
}